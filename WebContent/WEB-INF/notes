	private String name;

	public String getName() {
		return this.firstName + " " + this.lastName;
	}

	public Follower(User userByFriendId, User userByUserId) {
		this.userByFriendId = userByFriendId;
		this.userByUserId = userByUserId;
	}

	public GroupUser(Group group, User user) {
		this.group = group;
		this.user = user;
	}